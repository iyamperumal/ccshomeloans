
@model CcsData.Models.Contact
@{
    ViewBag.Title = "Contact";
}


<div id="wrapper">

    <!-- PAGE TITLE -->
    <section class="page-title" style="background: url('/content/images/page-title.jpg') center top;">
        <!-- for no background image, remove: style="background:url('assets/images/page-title.jpg') center top;" -->
        <span class="raster hover"><!-- raster --></span>

        <div class="container">

            <!-- APPLY BUTTON -->
            <div class="button-apply">

                @*<li class="@ViewBag.ApplyOnNowClass">@Html.ActionLink("Apply Now", "Create", "Applications4")</li>*@

                @Html.ActionLink("Apply Now", "App1", "Applications4", null, new { @class = "btn btn-lg btn-primary black" })
                <br /><span class="learn_more">or @Html.ActionLink("Learn more", "LearnMore", "Home")</span>
            </div>

            <!-- PAGE TITLE -->
            <header>
                <h1>CONTACT US</h1>
                <p>A Consumer Driven Instant Multiple Loan Quote Engine</p>
            </header>

            <!-- BREADCRUMBS - PAGE LOCATOR -->
            <ol class="breadcrumb">
                <li><a href="index.html">Home</a></li>
                <li>Contact Us</li>
            </ol>

        </div>
    </section>
    <!-- /PAGE TITLE -->
    <!-- Contact -->
    <section>
        <div class="container">

            <header>
                <h2 class="styleColor">Send us a Message</h2>
            </header>

            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="row">
                    <div class="form-inline">
                        <div class="form-group col-sm-4 col-md-4">
                            <label>Full Name</label>
                            @*<input type="text" name="full-name" class="form-control input-lg" placeholder="John Dow">*@
                            @Html.TextBoxFor(model => model.FullName, new { htmlAttributes = new { @class = "form-control input-lg" } })
                            @Html.ValidationMessageFor(model => model.FullName, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group col-sm-4 col-md-4">
                            <label>Email Address</label>
                            @*<input type="text" name="mail" class="form-control input-lg" placeholder="you@youremail.com">*@
                            @Html.TextBoxFor(model => model.EmailAddress, new { htmlAttributes = new { @class = "form-control input-lg" } })
                            @Html.ValidationMessageFor(model => model.EmailAddress, "", new { @class = "text-danger" })

                        </div>
                        <div class="form-group col-sm-4 col-md-4">
                            <label>Subject</label>
                            @*<input type="text" name="subject" class="form-control input-lg" placeholder="Subject">*@
                            @Html.TextBoxFor(model => model.Subject, new { htmlAttributes = new { @class = "form-control input-lg" } })
                            @Html.ValidationMessageFor(model => model.Subject, "", new { @class = "text-danger" })
                        </div>
                    </div>
                </div><br>

                <div class="row">
                    <div class="textarea form-group col-sm-12 col-md-12">
                        <label>Message</label>
                        <textarea class="form-control input-lg" id="Message" name="Message" rows="6" placeholder="Your Message"></textarea>
                    </div>

                    <div class="form-group col-md-3">
                        <button class="btn btn-primary btn-lg btn-block" type="submit">SUBMIT YOUR MESSAGE</button>
                    </div>
                </div>

            }

        </div>
    </section>



